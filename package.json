{
  "name": "react-odontogram-3d",
  "version": "1.0.0",
  "description": "Interactive 3D dental chart component for React with TypeScript support",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "vite",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run type-check && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dealverina/react-odontogram-3d.git"
  },
  "keywords": [
    "react",
    "typescript",
    "dental",
    "odontogram",
    "3d",
    "three.js",
    "dental-chart",
    "teeth",
    "healthcare",
    "medical"
  ],
  "author": "Dea Alverina <dealverina@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dealverina/react-odontogram-3d/issues"
  },
  "homepage": "https://github.com/dealverina/react-odontogram-3d#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@react-three/fiber": "^8.18.0",
    "three": "^0.178.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/testing-library__react": "^10.0.1",
    "@types/three": "^0.157.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "postcss": "^8.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  }
}
